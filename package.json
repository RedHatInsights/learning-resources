    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "4.1.5",
    "ts-jest": "^29.1.1",
    "ts-patch": "^3.1.2",
    "typescript": "^5.3.3",
    "webpack-bundle-analyzer": "4.10.1"
  },
  "insights": {
    "appname": "learning-resources"
  }
